  "devDependencies": {
    "del": "^7.0.0",
    "gulp": "^4.0.2",
    "gulp-imagemin": "^8.0.0",
    "gulp-replace": "^1.1.3",
    "gulp-webp": "^4.0.1",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
